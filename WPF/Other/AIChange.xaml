<Window x:Class="MythicMobs_edit.WPF.Other.AIChange"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MythicMobs_edit.WPF.Mob"
        mc:Ignorable="d"
        Title="AI修改" Height="70" Width="260" WindowStartupLocation="CenterOwner" ResizeMode="NoResize" Icon="/MythicMobs_edit;component/icon.ico">
    <Grid Background="Black">
        <Label Content="AI" HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Name="AI_list"
                Style="{StaticResource Label}"/>

        <TextBox HorizontalAlignment="Left" Height="23" Margin="60,1,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185"
                 x:Name="AI" Text="{Binding b}" Style="{StaticResource TextBox}"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="27,1,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="28"
                 x:Name="AI_L" Text="{Binding a}" Style="{StaticResource TextBox}"/>

    </Grid>
</Window>
