<Window x:Class="MythicMobs_edit.WPF.Mob.Other.AIChange"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MythicMobs_edit.WPF.Mob.Other"
        mc:Ignorable="d"
        Title="AI修改" Height="90" Width="262" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Icon="/MythicMobs_edit;component/image/方的7.ico" Closing="Window_Closing">
    <Grid Background="Black" Margin="0,0,0,0">
        <Label Content="AI" HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Name="AI_list"
                Style="{StaticResource Label}"/>

        <TextBox HorizontalAlignment="Left" Height="23" Margin="60,1,0,0" VerticalAlignment="Top" Width="185"
                 x:Name="AI" Text="{Binding AI_.Type}" Style="{StaticResource TextBox}"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="27,1,0,0" VerticalAlignment="Top" Width="28"
                 x:Name="AI_L" Text="{Binding AI_.Number}" Style="{StaticResource TextBox}"/>
        <Button Content="OK" HorizontalAlignment="Left" Margin="170,29,0,0" VerticalAlignment="Top" Width="75" Style="{StaticResource Button}" IsDefault="True" Click="Button_Click"/>

    </Grid>
</Window>
