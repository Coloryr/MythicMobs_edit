<UserControl x:Class="MythicMobs_edit.WPF.Mob.ENDERMAN"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MythicMobs_edit.WPF.Mob"
             mc:Ignorable="d" Height="300" Width="290">
    <Grid>
        <Label Content="末影人设置" HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Width="90"/>
        <CheckBox Content="是否阻止传送" HorizontalAlignment="Left" Height="15" Margin="10,25,0,0" VerticalAlignment="Top" Width="100"
                  IsChecked="{Binding obj.PreventTeleport}"/>

    </Grid>
</UserControl>
